<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <settings>
    <minimums-mode      type="string"></minimums-mode>
    <minimums           type="int">200</minimums>
    <nav-source         type="string">NAV1</nav-source>
    <minimums           type="int">200</minimums>
    <target-roll-deg    type="double">0</target-roll-deg>
    <target-pitch-deg   type="double">0</target-pitch-deg>
    <vertical-speed-fpm type="int">0</vertical-speed-fpm>
    <bank-limit         type="int">47</bank-limit>
    <target-speed-kt    type="int">480</target-speed-kt>
    <target-speed-mach  type="double">0.40</target-speed-mach>
    <changeover         type="bool">0</changeover>
    <heading-bug-deg    type="int">0</heading-bug-deg>
    <vertical-speed-fpm type="int">0</vertical-speed-fpm>
    <selected-crs       type="int">0</selected-crs>
    <asel               type="int">100</asel>
    <tf-mode            type="int">8</tf-mode> <!-- Larger values give smoother results -->
  </settings>

  <locks>
    <heading            type="string">ROLL</heading>
    <altitude           type="string">PTCH</altitude>
    <speed              type="string"></speed>
    <heading-arm        type="string"></heading-arm>
    <altitude-arm       type="string"></altitude-arm>
    <AP-status          type="string">TEMP DISENGAGE</AP-status>
    <FD-status          type="bool">true</FD-status>
    <yaw-damper         type="bool">false</yaw-damper>
    <m-trim             type="bool">false</m-trim>
    <passive-mode       type="bool">false</passive-mode>
  </locks>

  <internal>
    <selected-crs       type="int">0</selected-crs>
    <target-climb-rate  type="int">0</target-climb-rate>
    <target-altitude    type="int">0</target-altitude>
    <target-pressure    type="double">0.0</target-pressure>
    <target-roll-deg    type="double">0.0</target-roll-deg>
    <nav-distance       type="double">0</nav-distance>
    <nav-type           type="string"></nav-type>
    <nav-id             type="string"></nav-id>
    <course-deflection  type="double">0.0</course-deflection>
    <gs-valid           type="bool">0</gs-valid>
    <gs-deflection      type="double">0.0</gs-deflection>
    <in-range           type="bool">0</in-range>
    <gs-in-range        type="bool">0</gs-in-range>
    <stab-target        type="double">0</stab-target>
    <cdi                type="double">0</cdi>
    <vert-speed-abs     type="double">0</vert-speed-abs>
  </internal>

</PropertyList>
